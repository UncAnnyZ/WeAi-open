{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "typeRoots": [
      "./types"
    ],
    "paths": {
      "*": ["./node_modules/*", "./types/*"]
    }
    },
    "include": [
    "./**/*.ts",
    "./**/*.tsx"
    ],
    "exclude": [
    "./node_modules"
    ]
}

